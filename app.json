{
  "name": "doro",
  "displayName": "doro",
  "android": {
    "package": "dev.hollowleaf.doro"
  },
  "expo": {
    "name": "doro",
    "slug": "doro",
    "scheme": "doro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Doro.png",
    "splash": {
      "image": "./assets/images/Doro.png",
      "resizeMode": "contain",
      "backgroundColor": "#191015"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "dev.hollowleaf.doro",
      "icon": "./assets/images/app-icon-android-legacy.png",
      "versionCode": "0.0.0",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tobi-foreground.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "./assets/images/Doro.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "ios": {
      "icon": "./assets/images/app-icon-ios.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.doro",
      "splash": {
        "image": "./assets/images/Doro.png",
        "tabletImage": "./assets/images/tobi-tablet.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "splash": {
        "image": "./assets/images/Doro.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      },
      "bundler": "metro"
    },
    "expo": {
      "extra": {
        "eas": {
          "projectId": "bdf6640b-480a-4c35-a7a7-fa15bc7eb82f"
        }
      }
    },
    "plugins": [
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  },
  "ignite": {
    "version": "9.1.0"
  }
}